CREATE TABLE [dbo].[SecurityPerfil]
(
	[IdModulo] [char](8) NOT NULL,
	[IdPerfil] [varchar](20) NOT NULL,
	[Descripcion] [varchar](50) NULL,
	[Sistema] [char](1) NULL,
	[Estado] [char](1) NULL,
	[UsuarioCreacion] [varchar](20) NULL,
	[FechaCreacion] [datetime] NULL,
	[UsuarioModificacion] [varchar](20) NULL,
	[FechaModificacion] [datetime] NULL,
	CONSTRAINT [PK_SecurityPerfil] PRIMARY KEY ([IdModulo],[IdPerfil]), 
	CONSTRAINT [FK_SecurityPerfil_CoreModulo] FOREIGN KEY ([IdModulo]) REFERENCES [CoreModulo]([IdModulo])
)
