	CONSTRAINT [PK_SecurityUsuario] PRIMARY KEY ([IdUsuario]), 
	CONSTRAINT [PK_SecurityUsuario_CorePersona] FOREIGN KEY ([IdPersona]) REFERENCES [CorePersona]([IdPersona])
