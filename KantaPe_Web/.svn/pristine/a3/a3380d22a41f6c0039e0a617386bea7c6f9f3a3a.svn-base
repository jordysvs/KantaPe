CREATE TABLE [dbo].[CoreParametro]
(
	[IdModulo] [char](8) NOT NULL,
	[IdParametro] [varchar](20) NOT NULL,
	[Descripcion] [varchar](100) NULL,
	[Valor] [varchar](100) NULL,
	[Sistema] [char](1) NULL,
	[Estado] [char](1) NULL,
	[UsuarioCreacion] [varchar](20) NULL,
	[FechaCreacion] [datetime] NULL,
	[UsuarioModificacion] [varchar](20) NULL,
	[FechaModificacion] [datetime] NULL,
	CONSTRAINT [PK_CoreParametro] PRIMARY KEY ([IdModulo],[IdParametro]), 
    CONSTRAINT [FK_CoreParametro_CoreModulo] FOREIGN KEY ([IdModulo]) REFERENCES [CoreModulo]([IdModulo]) 
)
