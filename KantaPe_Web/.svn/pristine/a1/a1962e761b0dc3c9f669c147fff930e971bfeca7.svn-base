CREATE TABLE [dbo].[CorePersona]
(
	[IdPersona] [int] NOT NULL,
	[ApellidoPaterno] [varchar](50) NULL,
	[ApellidoMaterno] [varchar](50) NULL,
	[Nombres] [varchar](50) NULL,
	[IdTipoDocumento] [int] NULL,
	[NumeroDocumento] [varchar](20) NULL,
	[Mail] [varchar](50) NULL,
	[Celular] [varchar](20) NULL,
	[Sistema] [char](1) NULL,
	[Estado] [char](1) NULL,
	[UsuarioCreacion] [varchar](20) NULL,
	[FechaCreacion] [datetime] NULL,
	[UsuarioModificacion] [varchar](20) NULL,
	[FechaModificacion] [datetime] NULL,
	CONSTRAINT [PK_CorePersona] PRIMARY KEY ([IdPersona]), 
    CONSTRAINT [FK_CorePersona_CoreTipoDocumento] FOREIGN KEY ([IdTipoDocumento]) REFERENCES [CoreTipoDocumento]([IdTipoDocumento]) 
)
